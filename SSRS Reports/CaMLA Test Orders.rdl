  <DataSets>
    <DataSet Name="AgentOrders">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ins_group, ins_name, ord.prd_id, prd_short_description, usr_login, usr_firstname, usr_surname, 
ino_licences_requested, ino_date_requested, ino_date_updated, ino_order_status from institution_licences_locations_orders ord
inner join institutions i
on i.ins_id = ord.ins_id
inner join products p
on p.prd_id = ord.prd_id
inner join users u
on u.usr_id = ord.usr_id
where (ins_group = @Institution OR @Institution = 'CaMLA')
and ord.prd_id in (@Product)
and ((ino_date_requested &gt;= @StartDate or @StartDate is null)
and (dateadd(day, -1, ino_date_requested) &lt;= @EndDate OR @EndDate is null))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
