  <DataSets>
    <DataSet Name="AccumulatedScores">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Project">
            <Value>=Parameters!Project.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH TotalScores AS
(
SELECT T.PRJ_ID, TIR.TIN_ID, TIN.TST_ID, TIN.TIN_WHEN_STARTED, TIN.TIN_USER_ID, SUM(TIR_SCORE) AS Total_Score from TEST_INSTANCE_RESPONSES TIR WITH (NOLOCK)
INNER JOIN TEST_INSTANCES TIN WITH (NOLOCK)
ON TIN.TIN_ID = TIR.TIN_ID
INNER JOIN TESTS T
ON T.TST_ID = TIN.TST_ID
GROUP BY T.PRJ_ID, TIR.TIN_ID, TIN.TST_ID, TIN.TIN_WHEN_STARTED, TIN.TIN_USER_ID
),
GroupScore AS
(
select tig.tin_id, sum(tig.tig_score) AS Group_Score from test_instance_score_groups tig WITH (NOLOCK)
inner join test_instances tin WITH (NOLOCK)
on tin.tin_id = tig.tin_id
inner join tests t
on t.tst_id = tin.tst_id
group by tig.tin_id
)
SELECT TS.PRJ_ID, TS.TST_ID, TS.TIN_ID, TS.TIN_WHEN_STARTED, TS.Total_Score, GS.Group_Score, U.USR_LOGIN, LTRIM(RTRIM(U.USR_FIRSTNAME + ' ' + U.USR_SURNAME)) AS CANDIDATE
FROM TotalScores TS
FULL JOIN GroupScore GS
ON GS.TIN_ID = TS.TIN_ID
INNER JOIN USERS U
ON U.USR_ID = TS.TIN_USER_ID
WHERE (@FromDate is null OR TS.TIN_WHEN_STARTED &gt;= @FromDate) AND (@ToDate is null OR TS.TIN_WHEN_STARTED &lt;= @ToDate)
AND TS.PRJ_ID LIKE @Project
ORDER BY TS.PRJ_ID, TS.TST_ID, TS.TIN_WHEN_STARTED, U.USR_LOGIN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
