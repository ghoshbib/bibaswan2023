  <DataSets>
    <DataSet Name="CentreProductEligibility">
      <Query>
        <DataSourceName>Pivotal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT session_qual_name, centre_number, centre_name, country, ee.rn_create_date, ee.start_date, ee.end_date from esol_eligibility ee
inner join centre c
on c.centre_id = ee.centre_session_products_id
WHERE country in (@Country)
AND ((ee.rn_create_date &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, ee.rn_create_date) &lt;= @EndDate OR @EndDate IS NULL))
order by session_qual_name, centre_number</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="session_qual_name">
          <DataField>session_qual_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="centre_number">
          <DataField>centre_number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="centre_name">
          <DataField>centre_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="country">
          <DataField>country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rn_create_date">
          <DataField>rn_create_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="start_date">
          <DataField>start_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="end_date">
          <DataField>end_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Countries">
      <Query>
        <DataSourceName>Pivotal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT Country
FROM            Centre CE
INNER JOIN COUNTRY CN
ON CN.COUNTRY_ID = CE.COUNTRY_ID
WHERE CN.SUB_CONTINENT IN (@Region)
ORDER BY Country</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="COUNTRY">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Region">
      <Query>
        <DataSourceName>Pivotal</DataSourceName>
