  </DataSources>
  <DataSets>
    <DataSet Name="Licences">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@insGroup">
            <Value>=Parameters!insGroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@productName">
            <Value>=Parameters!productName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH totals AS (
	SELECT	ii.ins_group,
			pr.prd_id,
			SUM	(	CASE	WHEN ir.ino_order_status IN
								('Allocated', 'Pending', 'Approved') 
							THEN ir.ino_licences_requested
						ELSE 0 END
			)	AS totalLicences
	FROM	institutions ii
		INNER JOIN	institution_licences_locations_orders ir
			ON	ii.ins_id = ir.ins_id
		INNER JOIN	products pr
			ON	ir.prd_id = pr.prd_id
	WHERE 	ii.ins_group IN (@insGroup)
		AND
		pr.prd_id IN (@productName)
	GROUP BY	ii.ins_group,
					pr.prd_id
),
usedtests AS (
	SELECT	ii.ins_group,
			pr.prd_id,
			ISNULL(SUM (ic.inl_licences_locked), 0) AS totalLocked
	FROM	institutions ii
		INNER JOIN	institution_licences_locations ic
			ON	ii.ins_id = ic.ins_id
		INNER JOIN	products pr
			ON	ic.prd_id = pr.prd_id
	GROUP BY	ii.ins_group,
				pr.prd_id
)

SELECT	tl.ins_group,
		ii.ins_name,
		pr.prd_id,
		pr.prd_short_description,
		tl.totalLicences,
		ut.totalLocked
FROM	totals tl
INNER JOIN	institutions ii
	ON	tl.ins_group = ii.ins_id
INNER JOIN	products pr
	ON	tl.prd_id = pr.prd_id 
INNER JOIN	usedtests ut
	ON	tl.ins_group = ut.ins_group
		AND
		tl.prd_id = ut.prd_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ins_group">
          <DataField>ins_group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="totalLicences">
          <DataField>totalLicences</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prd_id">
          <DataField>prd_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ins_name">
          <DataField>ins_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prd_short_description">
          <DataField>prd_short_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="totalLocked">
          <DataField>totalLocked</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="InstitutionGroups">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT DISTINCT	ii.ins_group
FROM		institutions ii
INNER JOIN	institution_licences_locations_orders ir
	ON	ii.ins_id = ir.ins_id
ORDER BY	ii.ins_group</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ins_group">
          <DataField>ins_group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT DISTINCT	pr.prd_id,
		pr.prd_short_description
FROM	institution_licences_locations_orders ir
INNER JOIN	products pr
	ON	ir.prd_id = pr.prd_id
ORDER BY	pr.prd_short_description ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prd_id">
          <DataField>prd_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prd_short_description">
          <DataField>prd_short_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
