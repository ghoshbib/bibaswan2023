  <DataSources>
    <DataSource Name="MetricaDeliveryLive">
      <DataSourceReference>MetricaDeliveryLive</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>84fc773d-4b67-4605-b527-ea3c5a8efe91</rd:DataSourceID>
