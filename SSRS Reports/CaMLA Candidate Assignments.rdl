  <DataSets>
    <DataSet Name="SessionTests">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Login">
            <Value>=Parameters!Login.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SES_REFERENCE, SES_START_DATE, S.SES_ID, SG.SGP_ID, SGP_NAME, SG.PRD_ID, PRD_SHORT_DESCRIPTION, INS_NAME, VEN_NAME, S.VEN_ID, USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, ASN_WHEN_CREATED, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED FROM ASSIGNMENTS A
INNER JOIN USERS U
ON U.USR_ID = A.USR_ID
LEFT JOIN SESSION_GROUPS SG
ON SG.SGP_ID = A.SGP_ID
LEFT JOIN SESSIONS S
ON S.SES_ID = A.SES_ID
LEFT JOIN PRODUCTS P
ON P.PRD_ID = SG.PRD_ID
INNER JOIN INSTITUTIONS I
ON I.INS_ID = S.INS_ID
INNER JOIN VENUES V
ON V.VEN_ID = S.VEN_ID
LEFT JOIN TEST_INSTANCES TIN
ON TIN_ASSIGNMENT_ID = A.ASN_ID
WHERE (I.INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND (USR_LOGIN LIKE @Login OR @Login = '')
AND SG.PRD_ID IN (@Product)
AND (TIN_WHEN_STARTED IS NULL AND 'N' IN (@Status)
OR TIN_WHEN_STARTED IS NOT NULL AND TIN_WHEN_SUBMITTED IS NULL AND 'S' IN (@Status)
OR TIN_WHEN_SUBMITTED IS NOT NULL AND 'C' IN (@Status))
ORDER BY USR_LOGIN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
