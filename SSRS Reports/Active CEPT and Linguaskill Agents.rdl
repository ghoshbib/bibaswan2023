  <DataSources>
    <DataSource Name="MetricaDeliveryLive">
      <DataSourceReference>MetricaDeliveryLive</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>a0851dfa-1253-47a4-a4eb-407267455302</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="CEPTActiveAgents">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@rolType">
            <Value>=Parameters!rolType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	ii.ins_id,
	ii.ins_name,
	us.usr_login,
	us.usr_firstname,
	us.usr_surname
FROM	institutions ii
INNER JOIN	elegibility eg
	ON	ii.ins_id = eg.ins_id
		AND
		eg.prd_id IN ('D171', 'D541')
INNER JOIN	users us
	ON	us.ins_id = ii.ins_id 
INNER JOIN	user_roles ro
	ON	us.usr_id = ro.usr_id
		AND	(
		(eg.prd_id = 'D171' AND ro.rol_id IN ('Agent', 'CEPT_Agent'))
		OR
		(eg.prd_id = 'D541' AND ro.rol_id = 'Linguaskill_Agent')
		)
WHERE	(us.usr_is_enabled = 1 or @Status = 'X')
	AND
	IIF(ro.rol_id='CEPT_Agent', 'Agent', ro.rol_id) IN (@rolType)
GROUP BY	ii.ins_id,
		ii.ins_name,
		us.usr_login,
		us.usr_firstname,
		us.usr_surname
ORDER BY	ins_id ASC,
	usr_login ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="INS_ID">
          <DataField>ins_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ins_name">
          <DataField>ins_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_login">
          <DataField>usr_login</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_firstname">
          <DataField>usr_firstname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_surname">
          <DataField>usr_surname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
