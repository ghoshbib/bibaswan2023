  <DataSets>
    <DataSet Name="CEPTActiveAgents">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct u.INS_ID, ins_name, usr_login, usr_firstname, usr_surname, usr_is_enabled from USERS u
inner join ELEGIBILITY e
on e.INS_ID = u.INS_ID
inner join USER_ROLES ur
on (ur.USR_ID = u.USR_ID and ROL_ID = 'Agent')
inner join INSTITUTIONS i
on i.INS_ID = u.ins_id
inner join products p
on p.prd_id = e.prd_id
where PFM_ID = 'Cambridge English Placement Test'
and USR_IS_STUDENT = 0
and (USR_IS_ENABLED = 1 or @Status = 'X')
order by INS_ID, usr_login</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="INS_ID">
          <DataField>INS_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ins_name">
          <DataField>ins_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_login">
          <DataField>usr_login</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_firstname">
          <DataField>usr_firstname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_surname">
          <DataField>usr_surname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="usr_is_enabled">
          <DataField>usr_is_enabled</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
