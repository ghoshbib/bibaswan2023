  <DataSets>
    <DataSet Name="AgentOrders">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH BTRANS AS
(
SELECT PRD_SHORT_DESCRIPTION AS PRD_DESC, B.BOK_WHEN_CREATED AS CREATE_DATE, B.CRC_ID, BL.BLN_QUANTITY AS LIC_QUANTITY, BL.BLN_PRICE, B.BOK_STATUS, I.INS_NAME,
U.USR_FIRSTNAME, U.USR_SURNAME FROM BOOKING_LINES BL WITH (NOLOCK)
LEFT JOIN BOOKINGS B WITH (NOLOCK)
ON BL.BOK_ID = B.BOK_ID
LEFT JOIN INSTITUTIONS I WITH (NOLOCK)
ON I.INS_ID = B.BOK_INSTITUTION
FULL JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = B.BOK_USER
FULL JOIN PRODUCTS PR WITH (NOLOCK)
ON PR.PRD_ID = BL.BLN_ITEM
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND I.INS_PAYMENTTYPE LIKE 'Invoiced%'
AND BOK_PAYMENT_ID IS NULL
AND BOK_STATUS &lt;&gt; 'TokenPaid'
AND ((B.BOK_WHEN_CREATED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, B.BOK_WHEN_CREATED) &lt;= @EndDate OR @EndDate IS NULL))
),
ATRANS AS
(
SELECT AUD_KEY2 AS PRD_DESC, AUD_WHEN_CREATED AS CREATE_DATE, AUD_CHANGE_TO AS LIC_QUANTITY, I.INS_NAME FROM AUDIT AUD WITH (NOLOCK)
LEFT JOIN INSTITUTIONS I WITH (NOLOCK)
ON I.INS_ID = AUD_KEY1
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND I.INS_PAYMENTTYPE LIKE 'Invoiced%'
AND AUD_OBJECT_NAME = 'Test'
AND AUD_FIELD = '[Audit_Test_AvailableLicences]'
AND ((AUD_WHEN_CREATED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, AUD_WHEN_CREATED) &lt;= @EndDate OR @EndDate IS NULL))
)
SELECT 'B' AS RECTYPE, PRD_DESC, CREATE_DATE, CRC_ID, LIC_QUANTITY, BLN_PRICE, BOK_STATUS, INS_NAME, USR_FIRSTNAME, USR_SURNAME FROM BTRANS
UNION ALL
SELECT 'A' AS RECTYPE, PRD_DESC, CREATE_DATE, NULL, LIC_QUANTITY, 0, 'Authorised', INS_NAME, 'Cambridge', 'Michigan' FROM ATRANS
ORDER BY CREATE_DATE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
