  <DataSets>
    <DataSet Name="FinanceInvoiced">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH BOOKINGDATA AS
(
SELECT PR.PRD_ID, PRD_SHORT_DESCRIPTION AS PRD_DESC, T.TST_ID, PR.PFM_ID, S.SES_START_DATE, B.BOK_USER, B.BOK_WHEN_CREATED, B.CRC_ID, B.BOK_TOTAL, B.BOK_ID,
B.BOK_PAYMENT_ID, B.BOK_TAX, B.BOK_PAYMENT_METHOD, BL.BLN_ITEM, BL.BLN_DESCRIPTION, BL.BLN_QUANTITY, BL.BLN_UNIT_PRICE, INS_DISCOUNT_AMOUNT, 'B' AS RECTYPE, BL.BLN_PRICE, BL.BLN_TAX_CODE,
B.BOK_STATUS, B.BOK_TYPE, B.BOK_REFERENCE, S.SES_REFERENCE, I.INS_NAME, I.INS_COUNTRY, I.INS_PAYMENTTYPE, I.INS_VAT_NUMBER, I.INS_ID, I.INS_GROUP, INS_AGENTORCHANNEL_PARTNER,
V.VEN_NAME, IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , U.USR_FIRSTNAME, ASN_SOR_FIRSTNAME) AS USR_FIRSTNAME, 
IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , U.USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME FROM BOOKING_LINES BL WITH (NOLOCK)
LEFT JOIN BOOKINGS B WITH (NOLOCK)
ON BL.BOK_ID = B.BOK_ID
LEFT JOIN ASSIGNMENTS A
ON A.ASN_ID = B.ASN_ID
LEFT JOIN INSTITUTIONS I WITH (NOLOCK)
ON I.INS_ID = B.BOK_INSTITUTION
FULL JOIN SESSIONS S WITH (NOLOCK)
ON S.SES_ID = B.SES_ID
FULL JOIN CONTENT C WITH (NOLOCK)
ON C.CNT_ID = S.CNT_ID
FULL JOIN TESTS T WITH (NOLOCK)
ON T.TST_ID = C.CNT_TEST_ID
FULL JOIN VENUES V WITH (NOLOCK)
ON V.VEN_ID = S.VEN_ID
FULL JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = B.BOK_USER
FULL JOIN PRODUCTS PR WITH (NOLOCK)
ON PR.PRD_ID = BL.BLN_ITEM
LEFT JOIN INSTITUTION_LICENCES IL
ON (IL.INS_ID = I.INS_ID AND IL.PRD_ID = PR.PRD_ID)
WHERE (PR.PRD_ID LIKE 'D54%' OR PR.PRD_ID LIKE 'D65%' OR BLN_ITEM LIKE 'Lingua%')
AND Format(BOK_WHEN_CREATED, 'MM/yyyy') = @Month
AND I.INS_PAYMENTTYPE LIKE 'Invoiced%'
AND BOK_PAYMENT_ID IS NULL
AND BOK_STATUS &lt;&gt; 'TokenPaid'
AND INS_GROUP NOT IN ('BDG', 'GB599')
AND INS_GROUP NOT LIKE '%office'
),
AUDITDATA AS
(
SELECT P.PRD_ID, AUD_KEY2 AS PRD_DESC, NULL AS TST_ID, PFM_ID, NULL AS SES_START_DATE, USR_ID AS BOK_USER, AUD_WHEN_CREATED AS BOK_WHEN_CREATED, I.CRC_ID, NULL AS BOK_TOTAL,
NULL AS BOK_ID, CAST(AUD_ID AS NVARCHAR(10)) AS BOK_PAYMENT_ID, NULL AS BOK_TAX, 'Adjustment' AS BOK_PAYMENT_METHOD, P.PRD_ID AS BLN_ITEM, AUD_KEY2 AS BLN_DESCRIPTION, CAST(AUD_CHANGE_TO AS INT) AS BLN_QUANTITY,
PRD_PRICE AS BLN_UNIT_PRICE, INS_DISCOUNT_AMOUNT, 'A' AS RECTYPE, NULL AS BLN_PRICE, NULL AS BLN_TAX_CODE, 'Audited' AS BOK_STATUS, NULL AS BOK_TYPE, CAST(AUD_ID AS NVARCHAR(10)) AS BOK_REFERENCE,
NULL AS SES_REFERENCE, INS_NAME, INS_COUNTRY, INS_PAYMENTTYPE, INS_VAT_NUMBER, I.INS_ID, INS_GROUP, INS_AGENTORCHANNEL_PARTNER, NULL AS VEN_NAME, USR_FIRSTNAME, USR_SURNAME FROM AUDIT A
INNER JOIN INSTITUTIONS I
ON I.INS_ID = AUD_KEY1
INNER JOIN PRODUCTS P
ON PRD_SHORT_DESCRIPTION = AUD_KEY2
LEFT JOIN PRODUCT_PRICES PP
ON (PP.PRD_ID = P.PRD_ID AND PP.CRC_ID = I.CRC_ID)
LEFT JOIN INSTITUTION_LICENCES IL
ON (IL.INS_ID = I.INS_ID AND IL.PRD_ID = P.PRD_ID)
LEFT JOIN USERS U
ON (U.USR_LOGIN = AUD_CHANGED_BY AND U.INS_ID = 'CambridgeEnglish')
WHERE (P.PRD_ID LIKE 'D54%' OR P.PRD_ID LIKE 'D65%' OR P.PRD_ID LIKE 'Lingua%')
AND Format(AUD_WHEN_CREATED, 'MM/yyyy') = @Month
AND AUD_FIELD = 'Available Test Credits'
)
SELECT * FROM BOOKINGDATA
UNION ALL
SELECT * FROM AUDITDATA
ORDER BY BOK_WHEN_CREATED</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
