  <DataSets>
    <DataSet Name="CEPTActiveAgents">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@rolType">
            <Value>=Parameters!rolType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Written by David Dursun */
/* Amended by Adrian Nicholl, August 2019: */
/* Addition of Linguaskill Business and YLPT */

SELECT	ii.ins_id,
	ii.ins_name,
	us.usr_login,
	us.usr_firstname,
	us.usr_surname,
	us.usr_email_address
FROM	institutions ii
INNER JOIN	elegibility eg
	ON	ii.ins_id = eg.ins_id
		AND
		eg.prd_id IN ('D171', 'D176', 'D541', 'D542', 'D544', 'D545', 'D651', 'D652', 'D654', 'D655')
INNER JOIN	users us
	ON	us.ins_id = ii.ins_id 
INNER JOIN	user_roles ro
	ON	us.usr_id = ro.usr_id
		AND	(
		('AC' IN (@rolType) and eg.prd_id = 'D171' AND ro.rol_id IN ('Agent', 'CEPT_Agent'))
		OR
		('AY' IN (@rolType) and eg.prd_id = 'D176' AND ro.rol_id IN ('Agent', 'CEPT_Agent'))
		OR
		('LG' IN (@rolType) and eg.prd_id LIKE 'D54%' AND ro.rol_id = 'Linguaskill_Agent')
		OR
		('LB' IN (@rolType) and eg.prd_id LIKE 'D65%' AND ro.rol_id = 'Linguaskill_Agent')
		)
WHERE	(us.usr_is_enabled = 1 or @Status = 'X')
	AND
	IIF(eg.prd_id = 'D171', 'AC', IIF(eg.prd_id = 'D176', 'AY', IIF(eg.prd_id LIKE 'D54%', 'LG', 'LB'))) IN (@rolType)
GROUP BY	ii.ins_id,
		ii.ins_name,
		us.usr_login,
		us.usr_firstname,
		us.usr_surname,
		us.usr_email_address
ORDER BY	ins_id ASC,
	usr_login ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
