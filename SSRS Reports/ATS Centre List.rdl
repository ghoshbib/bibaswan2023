  <DataSets>
    <DataSet Name="Eligibility">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT INS_GROUP, INS_NAME, INS_ADDRESS, CNT_NAME FROM ELEGIBILITY E WITH (NOLOCK)
INNER JOIN INSTITUTIONS I WITH (NOLOCK)
ON I.INS_ID = E.INS_ID
INNER JOIN COUNTRIES C
ON C.CNT_ID = INS_COUNTRY
INNER JOIN PRODUCTS P WITH (NOLOCK)
ON P.PRD_ID = E.PRD_ID
WHERE E.PRD_ID IN (@Product)
AND INS_STATUS = 'Active'
ORDER BY INS_GROUP</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="INS_GROUP">
          <DataField>INS_GROUP</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INS_NAME">
          <DataField>INS_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INS_ADDRESS">
          <DataField>INS_ADDRESS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CNT_NAME">
          <DataField>CNT_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT PRD_ID, PRD_SHORT_DESCRIPTION, PRDNAME = 
 CASE
  WHEN PRD_SHORT_DESCRIPTION IS NULL THEN PRD_ID
  ELSE PRD_ID + ' / ' + PRD_SHORT_DESCRIPTION
 END
FROM PRODUCTS WITH (NOLOCK)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PRD_ID">
          <DataField>PRD_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRD_SHORT_DESCRIPTION">
          <DataField>PRD_SHORT_DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRDNAME">
          <DataField>PRDNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
