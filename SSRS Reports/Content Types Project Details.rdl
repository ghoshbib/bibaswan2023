  <DataSets>
    <DataSet Name="ItemContentTypes">
      <Query>
        <DataSourceName>MetricaAdminLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContentType">
            <Value>=Parameters!ContentType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT I.PRJ_ID, I.CTY_ID, COUNT(I.ITM_ID) AS "Occurances" FROM ITEMS I
INNER JOIN SECTION_ITEMS S
ON S.ITM_ID = I.ITM_ID
WHERE CTY_ID = @ContentType
GROUP BY I.PRJ_ID, I.CTY_ID
ORDER BY I.PRJ_ID, I.CTY_ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CTY_ID">
          <DataField>CTY_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRJ_ID">
          <DataField>PRJ_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Occurances">
          <DataField>Occurances</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
