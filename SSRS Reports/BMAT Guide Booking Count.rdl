  <DataSets>
    <DataSet Name="BMATGuide">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with guideusers as
(
select distinct bln_description, bok_user, usr_login, usr_email_address from BOOKING_LINES bl with (nolock)
inner join BOOKINGS b with (nolock)
on b.BOK_ID = bl.BOK_ID
inner join users u with (nolock)
on u.usr_id = bok_user
where BLN_DESCRIPTION like '%BMAT Section 2%'
AND ((BOK_WHEN_CREATED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, BOK_WHEN_CREATED) &lt;= @EndDate OR @EndDate IS NULL))
)
select bln_description, count(*) as BookCount from guideusers
group by bln_description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="bln_description">
          <DataField>bln_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BookCount">
          <DataField>BookCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
