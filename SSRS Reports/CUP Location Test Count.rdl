  <DataSets>
    <DataSet Name="TestCounts">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT INS_GROUP, INS_NAME, ISNULL(CNT_TEST_ID, '') AS CNT_TEST_ID, COUNT(*) AS TESTCOUNT FROM INSTITUTIONS I
LEFT JOIN ASSIGNMENTS A
ON A.INS_ID = I.INS_ID
LEFT JOIN SESSIONS S
ON S.SES_ID = A.SES_ID
LEFT JOIN SESSION_GROUPS SG
ON SG.SGP_ID = A.SGP_ID
LEFT JOIN PRODUCTS P
ON P.PRD_ID = SG.PRD_ID
LEFT JOIN CONTENT C
ON C.CNT_ID = S.CNT_ID
WHERE ((INS_NAME LIKE '%CUP%' OR INS_NAME LIKE '%Press%')
AND INS_GROUP &lt;&gt; 'CEdemo')
AND PFM_ID = 'Cambridge English Placement Test'
GROUP BY INS_GROUP, INS_NAME, CNT_TEST_ID 
ORDER BY INS_GROUP, INS_NAME</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="INS_GROUP">
          <DataField>INS_GROUP</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
