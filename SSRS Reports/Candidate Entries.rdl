  <DataSets>
    <DataSet Name="BMATCandidateEntries">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT A.ASN_ID, U.USR_LOGIN, 
IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , U.USR_FIRSTNAME, ASN_SOR_FIRSTNAME) AS USR_FIRSTNAME, 
IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , U.USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME, 
A.INS_ID, I.INS_GROUP
FROM ASSIGNMENTS A
INNER JOIN USERS U
ON U.USR_ID = A.USR_ID
INNER JOIN SESSIONS S
ON S.SES_ID = A.SES_ID
INNER JOIN INSTITUTIONS I
ON I.INS_ID = A.INS_ID
WHERE S.SES_REFERENCE LIKE @Session
ORDER BY U.USR_LOGIN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
