  <DataSources>
    <DataSource Name="MetricaDelivery">
      <ConnectionProperties>
        <DataProvider>SQLAZURE</DataProvider>
        <ConnectString>Data Source=pmetreplicasqlazure001.database.windows.net;Initial Catalog=metrica;Encrypt=True;TrustServerCertificate=False</ConnectString>
        <Prompt>Specify a user name and password for data source MetricaDelivery:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>c6d5b26b-8e45-4569-b55c-36f560ad3f3d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="audio_quality">
      <Query>
        <DataSourceName>MetricaDelivery</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@a">
            <Value>="&lt;n&gt;" + join(Parameters!filename.Value,"&lt;/n&gt;&lt;n&gt;") + "&lt;/n&gt;"</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @p xml = @a;
WITH flies(fname) AS (SELECT CAST(p.query('./text()') AS varchar(255))
FROM @p.nodes('/n') AS T(p)),
patterned AS (
  SELECT  fname,
      SUBSTRING(fname, 0, CHARINDEX('__', fname)) AS tin_id,
      CONCAT('[audio', 
        SUBSTRING(fname, 
          CHARINDEX('__', fname) + 2, 
          LEN(fname) - CHARINDEX('__', fname) - 5), ']' )
        AS pattern
  FROM  flies
)
SELECT  p1.fname,
    IIF(tr.tir_data='', 'fine', tr.tir_data) AS quality
FROM  patterned p1
INNER JOIN  test_instance_responses tr
  ON  p1.tin_id = tr.tin_id
    AND
    tr.tir_response = p1.pattern
;</CommandText>
      </Query>
      <Fields>
        <Field Name="fname">
          <DataField>fname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="quality">
          <DataField>quality</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
