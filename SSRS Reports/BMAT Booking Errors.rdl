    <DataSet Name="BMATLeidenCandidates">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT USR_LOGIN, IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME, ATT_VALUE FROM ASSIGNMENTS A WITH (NOLOCK)
INNER JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = A.USR_ID
INNER JOIN SESSIONS S WITH (NOLOCK)
ON S.SES_ID = A.SES_ID
LEFT JOIN CUSTOM_ATTRIBUTES CA WITH (NOLOCK)
ON (CA.USR_ID = A.USR_ID) AND (CA.ATT_NAME = 'LUMCID')
WHERE SES_REFERENCE = 'BMAT Leiden Candidates'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="USR_LOGIN">
          <DataField>USR_LOGIN</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="USR_SURNAME">
          <DataField>USR_SURNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ATT_VALUE">
          <DataField>ATT_VALUE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
