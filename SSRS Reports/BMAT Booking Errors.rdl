  <DataSets>
    <DataSet Name="BMATLeidenBookings">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>with Success as
(
select bok_user, bok_id, usr_login, usr_firstname, usr_surname, ltrim(rtrim(IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , USR_FIRSTNAME, ASN_SOR_FIRSTNAME) + ' ' + IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME))) as Fullname, IIF(ISNULL(ASN_SOR_EMAIL_ADDRESS, '') = '' , USR_EMAIL_ADDRESS, ASN_SOR_EMAIL_ADDRESS) AS USR_EMAIL_ADDRESS, bok_when_created, bok_when_updated, bok_status, bok_payment_id from BOOKINGS b WITH (NOLOCK)
inner join USERS u WITH (NOLOCK)
on u.USR_ID = bok_user
left join assignments a
on a.asn_id = b.asn_id
inner join SESSIONS S WITH (NOLOCK)
on s.SES_ID = b.SES_ID
where s.SES_REFERENCE = 'BMAT Leiden 2015'
and bok_status = ('PaymentRequested')
),
Pending as
(
select bok_user, bok_id, usr_login, usr_firstname, usr_surname, ltrim(rtrim(usr_firstname + ' ' + usr_surname)) as Fullname, IIF(ISNULL(ASN_SOR_EMAIL_ADDRESS, '') = '' , USR_EMAIL_ADDRESS, ASN_SOR_EMAIL_ADDRESS) AS USR_EMAIL_ADDRESS, bok_when_created, bok_when_updated, bok_status from BOOKINGS b WITH (NOLOCK)
inner join USERS u WITH (NOLOCK)
on u.USR_ID = bok_user
LEFT JOIN ASSIGNMENTS A
ON A.ASN_ID = B.ASN_ID
inner join SESSIONS S WITH (NOLOCK)
on s.SES_ID = b.SES_ID
where s.SES_REFERENCE = 'BMAT Leiden 2015'
and bok_status &lt;&gt; ('PaymentRequested')
)
select s.* from Pending p
inner join Success s
on s.bok_user = p.bok_user
where p.bok_when_created &gt; s.bok_when_created</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
