  <DataSets>
    <DataSet Name="Reports">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@institution">
            <Value>=Parameters!institution.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with groups as
(
select distinct rgp_id from role_report_groups
where rol_id in
 (
  select distinct rol_id from user_roles
  where usr_id in
  (
   select usr_id from users
   where ins_id = @institution
  )
 )
)
select rgp_name, rpt_name, rpt_description from groups g
inner join report_groups rg
on rg.rgp_id = g.rgp_id
inner join report_group_reports rgr
on rgr.rgp_id = g.rgp_id
inner join reports r
on r.rpt_id = rgr.rpt_id
order by rgp_name, rpt_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="rgp_name">
          <DataField>rgp_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rpt_name">
          <DataField>rpt_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rpt_description">
          <DataField>rpt_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Institutions">
      <Query>
