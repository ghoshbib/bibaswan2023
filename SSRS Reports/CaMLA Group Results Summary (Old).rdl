  <DataSets>
    <DataSet Name="ResponsesScores">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SessionGroup">
            <Value>=Parameters!SessionGroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Session">
            <Value>=Parameters!Session.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Candidate">
            <Value>=Parameters!Candidate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH LISTENING AS
(
SELECT USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, (LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) AS CAND_NAME, U.USR_ID, USR_EMAIL_ADDRESS, TIN.TIN_ID, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED, TST_ID, SES_REFERENCE, SUM(TIR_SCORE) AS LSCORE FROM TEST_INSTANCE_RESPONSES TIR WITH (NOLOCK)
INNER JOIN TEST_INSTANCES TIN WITH (NOLOCK)
ON TIN.TIN_ID = TIR.TIN_ID
INNER JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = TIN_USER_ID
INNER JOIN ASSIGNMENTS A WITH (NOLOCK)
ON ASN_ID = TIN_ASSIGNMENT_ID
INNER JOIN SESSIONS S WITH (NOLOCK)
ON S.SES_ID = A.SES_ID
INNER JOIN INSTITUTIONS I
ON I.INS_ID = S.INS_ID
INNER JOIN SESSION_GROUPS SG
ON SG.SGP_ID = A.SGP_ID
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND TIN_WHEN_SUBMITTED &gt;= '2015-09-01'
AND ITM_ID &lt;&gt; ''
AND SG.PRD_ID IN (@Product)
AND (SGP_NAME = @SessionGroup OR @SessionGroup = '')
AND (SES_REFERENCE = @Session OR @Session = '')
AND TIR_SECTION LIKE '%Listening%'
AND ((LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) LIKE @Candidate OR @Candidate = '')
AND ((TIN_WHEN_SUBMITTED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, TIN_WHEN_SUBMITTED) &lt;= @EndDate OR @EndDate IS NULL))
GROUP BY USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, (LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))), U.USR_ID, USR_EMAIL_ADDRESS, TIN.TIN_ID, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED, TST_ID, SES_REFERENCE
),
GVR AS
(
SELECT USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, (LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) AS CAND_NAME, U.USR_ID, USR_EMAIL_ADDRESS, TIN.TIN_ID, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED, TST_ID, SES_REFERENCE, SUM(TIR_SCORE) AS GSCORE FROM TEST_INSTANCE_RESPONSES TIR WITH (NOLOCK)
INNER JOIN TEST_INSTANCES TIN WITH (NOLOCK)
ON TIN.TIN_ID = TIR.TIN_ID
INNER JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = TIN_USER_ID
INNER JOIN ASSIGNMENTS A WITH (NOLOCK)
ON ASN_ID = TIN_ASSIGNMENT_ID
INNER JOIN SESSIONS S WITH (NOLOCK)
ON S.SES_ID = A.SES_ID
INNER JOIN INSTITUTIONS I
ON I.INS_ID = S.INS_ID
INNER JOIN SESSION_GROUPS SG
ON SG.SGP_ID = A.SGP_ID
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND TIN_WHEN_SUBMITTED &gt;= '2015-09-01'
AND ITM_ID &lt;&gt; ''
AND SG.PRD_ID IN (@Product)
AND (SGP_NAME = @SessionGroup OR @SessionGroup = '')
AND (SES_REFERENCE = @Session OR @Session = '')
AND (TIR_SECTION LIKE '%GVR%' OR TIR_SECTION LIKE '%GCVR%' OR TIR_SECTION LIKE '%GVCR%')
AND ((LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) LIKE @Candidate OR @Candidate = '')
AND ((TIN_WHEN_SUBMITTED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, TIN_WHEN_SUBMITTED) &lt;= @EndDate OR @EndDate IS NULL))
GROUP BY USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, (LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))), U.USR_ID, USR_EMAIL_ADDRESS, TIN.TIN_ID, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED, TST_ID, SES_REFERENCE
),
TOTAL AS
(
SELECT USR_LOGIN, USR_FIRSTNAME, USR_SURNAME, (LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) AS CAND_NAME, U.USR_ID, USR_EMAIL_ADDRESS, TIN.TIN_ID, TIN_WHEN_STARTED, TIN_WHEN_SUBMITTED, TST_ID, SES_REFERENCE, AGG1.AGG_VALUE AS TOTAL_SCORE, AGG2.AGG_VALUE AS TOTAL_GRADE FROM TEST_INSTANCES TIN WITH (NOLOCK)
INNER JOIN AGGREGATIONS AGG1
ON (AGG1.TIN_ID = TIN.TIN_ID AND AGG1.AGG_ID = 'Total_Score')
INNER JOIN AGGREGATIONS AGG2
ON (AGG2.TIN_ID = TIN.TIN_ID AND AGG2.AGG_ID = 'Total_Grade')
INNER JOIN USERS U WITH (NOLOCK)
ON U.USR_ID = TIN_USER_ID
INNER JOIN ASSIGNMENTS A WITH (NOLOCK)
ON ASN_ID = TIN_ASSIGNMENT_ID
INNER JOIN SESSIONS S WITH (NOLOCK)
ON S.SES_ID = A.SES_ID
INNER JOIN INSTITUTIONS I
ON I.INS_ID = S.INS_ID
INNER JOIN SESSION_GROUPS SG
ON SG.SGP_ID = A.SGP_ID
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
AND TIN_WHEN_SUBMITTED &gt;= '2015-09-01'
AND SG.PRD_ID IN (@Product)
AND (SGP_NAME = @SessionGroup OR @SessionGroup = '')
AND (SES_REFERENCE = @Session OR @Session = '')
AND ((LTRIM(RTRIM(USR_FIRSTNAME)) + ' ' + LTRIM(RTRIM(USR_SURNAME))) LIKE @Candidate OR @Candidate = '')
AND ((TIN_WHEN_SUBMITTED &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, TIN_WHEN_SUBMITTED) &lt;= @EndDate OR @EndDate IS NULL))
)
SELECT L.USR_LOGIN AS LLOGIN, L.USR_FIRSTNAME AS LFIRST, L.USR_SURNAME AS LSURN, L.CAND_NAME AS LCAND, L.USR_ID AS LUSR, L.USR_EMAIL_ADDRESS AS LEMAIL, L.TIN_ID AS LTIN, L.TIN_WHEN_STARTED AS LSTART, L.TIN_WHEN_SUBMITTED AS LSUB, L.TST_ID AS LTST, L.SES_REFERENCE AS LSES, G.USR_LOGIN AS GLOGIN, G.USR_FIRSTNAME AS GFIRST, G.USR_SURNAME AS GSURN, G.CAND_NAME AS GCAND, G.USR_ID AS GUSR, G.USR_EMAIL_ADDRESS AS GEMAIL, G.TIN_ID AS GTIN, G.TIN_WHEN_STARTED AS GSTART, G.TIN_WHEN_SUBMITTED AS GSUB, G.TST_ID AS GTST, G.SES_REFERENCE AS GSES, LSCORE, GSCORE, TOTAL_SCORE, TOTAL_GRADE FROM LISTENING L
FULL JOIN GVR G
ON G.TIN_ID = L.TIN_ID
FULL JOIN TOTAL T
ON T.TIN_ID = L.TIN_ID</CommandText>
