  <DataSets>
    <DataSet Name="ConfigTests">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH AGENT_DATA AS
(
SELECT LIC1.INS_ID, ins_name, INS_GROUP, LIC1.PRD_ID, INS_AVAILABLE_LICENCES, INS_LICENCES_USED, INS_LICENCES_LOCKED, INS_ORDER_LIMIT, INL_AVAILABLE_LICENCES AS ALLOCATED FROM INSTITUTION_licences lic1
inner join institutions i
on i.ins_id = lic1.ins_id
LEFT JOIN INSTITUTION_LICENCES_LOCATIONS LIC2
ON (LIC2.INS_ID = LIC1.INS_ID AND LIC2.PRD_ID = LIC1.PRD_ID)
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
),
CLIENT_DATA AS
(
SELECT LIC.INS_ID, ins_name, INS_GROUP, LIC.PRD_ID, INL_LICENCES_USED, INL_LICENCES_LOCKED, INL_AVAILABLE_LICENCES AS ALLOCATED FROM INSTITUTION_licences_locations LIC
inner join institutions i
on i.ins_id = LIC.ins_id
WHERE (INS_GROUP = @Institution OR @Institution = 'CaMLA')
)
SELECT 'A' AS RECTYPE, INS_NAME, PRD_ID, INS_AVAILABLE_LICENCES, ALLOCATED, INS_LICENCES_USED, INS_LICENCES_LOCKED, INS_ORDER_LIMIT FROM AGENT_DATA A
UNION ALL
SELECT 'C' AS RECTYPE, INS_NAME, PRD_ID, NULL, ALLOCATED, INL_LICENCES_USED, INL_LICENCES_LOCKED, NULL FROM CLIENT_DATA C
ORDER BY PRD_ID, RECTYPE, INS_NAME</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RECTYPE">
          <DataField>RECTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INS_NAME">
          <DataField>INS_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRD_ID">
          <DataField>PRD_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INS_AVAILABLE_LICENCES">
          <DataField>INS_AVAILABLE_LICENCES</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ALLOCATED">
          <DataField>ALLOCATED</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="INS_LICENCES_USED">
          <DataField>INS_LICENCES_USED</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
