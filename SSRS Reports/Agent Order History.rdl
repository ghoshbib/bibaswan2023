    <DataSet Name="Products">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT P.PRD_ID, PRD_SHORT_DESCRIPTION, PROD =
 CASE ISNULL(P.PRD_SHORT_DESCRIPTION, '')
  WHEN '' THEN P.PRD_ID
  ELSE PRD_SHORT_DESCRIPTION
 END
 FROM PRODUCTS P
INNER JOIN INSTITUTION_LICENCES ORD
ON ORD.PRD_ID = P.PRD_ID
WHERE ORD.INS_ID IN (@Institution)
ORDER BY PROD</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PRD_ID">
          <DataField>PRD_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRD_SHORT_DESCRIPTION">
          <DataField>PRD_SHORT_DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PROD">
          <DataField>PROD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Institutions">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT DISTINCT I2.INS_ID, I2.INS_NAME FROM INSTITUTION_LICENCES ORD
INNER JOIN INSTITUTIONS I1
ON I1.INS_ID = ORD.INS_ID
INNER JOIN INSTITUTIONS I2
ON I2.INS_ID = I1.INS_GROUP
ORDER BY I2.INS_ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
