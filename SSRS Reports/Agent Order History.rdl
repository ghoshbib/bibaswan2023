  <DataSets>
    <DataSet Name="OrderHistory">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select i1.ins_group as InstID, i2.ins_name as Institution, cnt_name, i1.ins_name As Location, ord.prd_id, prd_short_description, ord.usr_id, LTRIM(RTRIM(usr_firstname + ' ' + usr_surname)) as AgentName,
usr_email_address, usr_when_created, ino_licences_requested, ino_date_requested, ino_date_updated, ino_order_status, ino_order_comments from institution_licences_locations_orders ord
inner join institutions i1
on i1.ins_id = ord.ins_id
inner join institutions i2
on i2.ins_id = i1.ins_group
inner join countries c
on c.cnt_id = i2.ins_country
inner join users u
on u.usr_id = ord.usr_id
inner join products p
on p.prd_id = ord.prd_id
where i1.ins_group in (@Institution)
and ord.prd_id in (@Product)
and ((ino_date_requested &gt;= @StartDate or @StartDate is null)
and (dateadd(day, -1, ino_date_requested) &lt;= @EndDate or @EndDate is null))
order by i1.ins_group, i1.ins_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
