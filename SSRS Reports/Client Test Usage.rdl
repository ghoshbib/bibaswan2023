  <DataSets>
    <DataSet Name="ClientAllocations">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Agent">
            <Value>=Parameters!Agent.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with clients as
(
select ins_group, ins_id, ins_name, ins_country, cnt_name  from institutions i
inner join countries c
on c.cnt_id = ins_country
where ins_group in (@Agent)
and i.ins_id &lt;&gt; ins_group
),
testreq as
(
select ins_group, c.ins_id, ins_name, cnt_name, ino.prd_id, prd_short_description, sum(ino_licences_requested) as licences_requested from clients c
left join institution_licences_locations_orders ino
on ino.ins_id = c.ins_id
left join products p
on p.prd_id = ino.prd_id
where ins_group in (@Agent)
and c.ins_id &lt;&gt; ins_group
and pfm_id = 'Cambridge English Placement Test'
and ((ino_date_requested &gt;= @StartDate OR @StartDate is null)
and (dateadd(day, -1, ino_date_requested) &lt;= @EndDate OR @EndDate is null))
group by ins_group, c.ins_id, ins_name, cnt_name, ino.prd_id, prd_short_description
)
select c.*, prd_id, prd_short_description, licences_requested from clients c
left join testreq t
on t.ins_id = c.ins_id
order by ins_group, ins_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
