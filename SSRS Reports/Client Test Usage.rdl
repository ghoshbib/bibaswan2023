    <DataSet Name="Agents">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT DISTINCT INS_GROUP, INS_NAME, INS_GROUP + ' (' + INS_NAME + ')' AS INST FROM INSTITUTIONS I
INNER JOIN ELEGIBILITY E
ON E.INS_ID = INS_GROUP
INNER JOIN PRODUCTS P
ON P.PRD_ID = E.PRD_ID
WHERE PFM_ID = 'Cambridge English Placement Test'
AND I.INS_ID = INS_GROUP
ORDER BY INS_GROUP</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="INS_GROUP">
          <DataField>INS_GROUP</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INS_NAME">
          <DataField>INS_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INST">
          <DataField>INST</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
