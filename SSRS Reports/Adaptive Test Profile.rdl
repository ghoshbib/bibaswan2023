  <DataSets>
    <DataSet Name="Product">
      <Query>
        <DataSourceName>Delivery</DataSourceName>
        <CommandText>SELECT  pr.prd_id,
    pr.prd_short_description
FROM  products pr
WHERE EXISTS (  SELECT  NULL 
        FROM  tests te 
        WHERE pr.prd_id = te.prd_id
            AND
            te.tty_id = 'Adaptive'
      )
;</CommandText>
      </Query>
      <Fields>
        <Field Name="prd_id">
          <DataField>prd_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prd_short_description">
          <DataField>prd_short_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Test">
      <Query>
        <DataSourceName>Delivery</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prd_id">
            <Value>=Parameters!prd_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	te.tst_id,
	te.tst_title
FROM	tests te
WHERE	te.prd_id IN (@prd_id)
;
	</CommandText>
      </Query>
      <Fields>
        <Field Name="tst_id">
          <DataField>tst_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tst_title">
          <DataField>tst_title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Test_Profile">
      <Query>
        <DataSourceName>Delivery</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@tst_id">
            <Value>=Parameters!tst_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@test_start">
            <Value>=Parameters!test_start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@skill">
            <Value>=Parameters!skill.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@test_end">
            <Value>=Parameters!test_end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH l2_tests AS (
  SELECT DISTINCT ti.tin_id,
      tr.tir_section,
      tr.itm_id
  FROM  test_instances ti
  INNER JOIN  test_instance_responses tr
    ON  ti.tin_id = tr.tin_id
  WHERE ti.tst_id IN (@tst_id)
      AND
      ti.tin_is_fully_marked = 1
      AND
      tr.tir_is_calibrated = 1
    AND
    (ti.tin_when_submitted &gt;= @test_start OR @test_start IS NULL)
    AND
    (ti.tin_when_submitted &lt;= DATEADD(d, 1, @test_end) OR @test_end IS NULL)
-- The below is so as to limit the number of rows, remove for PRD -- 
--      AND
--      ti.tin_when_submitted &gt;= '20181101'
),
agg_dict AS (
  SELECT	'Listening' AS skill, 'Listening_grade' AS agg_id
  UNION ALL
  SELECT	'Reading', 'Reading_grade'
),
agg_id_set AS (
  SELECT	*
  FROM	agg_dict
  WHERE	skill IN (@skill)
),
l2_aggs AS (
  SELECT  l2.tin_id,
      ag.agg_value AS cefr_level
  FROM  ( SELECT DISTINCT tin_id 
        FROM  l2_tests) l2
  INNER JOIN  aggregations ag
    ON  l2.tin_id = ag.tin_id
            AND
            ag.agg_id IN (SELECT agg_id FROM agg_id_set)
),
details AS (
  SELECT  l1.tin_id,
      l1.itm_id,
      it.itm_interaction_count,
      ia.att_id,
      ia.iat_value,
      l3.cefr_level,
      CASE  l3.cefr_level 
        WHEN  'Not Reported'  THEN 0
        WHEN  'Below A1'    THEN 1
        WHEN  'A1'      THEN 2
        WHEN  'A2'      THEN 3
        WHEN  'B1'      THEN 4
        WHEN  'B2'      THEN 5
        WHEN  'C1 or above' THEN 6
      END AS cefr_order
  FROM  l2_tests l1
  INNER JOIN  items it
    ON  l1.itm_id = it.itm_id
  INNER JOIN  item_attributes ia
    ON  l1.itm_id = ia.itm_id
      AND
      it.itm_current_version = ia.itv_version
      AND
      ia.att_id IN ('Difficulty', 'Task Type', 'Component')
  INNER JOIN  l2_aggs l3
    ON  l1.tin_id = l3.tin_id
),
pivots AS (
  SELECT  d1.tin_id,
      d1.itm_id,
      d1.itm_interaction_count,
      d1.cefr_level,
      d1.cefr_order,
      MAX(CASE WHEN d1.att_id = 'Difficulty'  THEN d1.iat_value END) AS difficulty,
      MIN(CASE WHEN d1.att_id = 'Task Type' THEN d1.iat_value END) AS task_type,
      MAX(CASE WHEN d1.att_id = 'Component' THEN d1.iat_value END) AS component
  FROM  details d1
  GROUP BY  d1.tin_id,
        d1.itm_id,
        d1.itm_interaction_count,
        d1.cefr_level,
        d1.cefr_order
),
means AS (
  SELECT  cefr_level,
      cefr_order,
      task_type,
      AVG(CAST(difficulty AS float)) AS mean_diff,
      CAST(SUM(itm_interaction_count) AS float) / CAST(COUNT(DISTINCT tin_id) AS float) AS mean_num_int
  FROM  pivots
  WHERE component IN (@skill)
  GROUP BY  cefr_level,
        cefr_order,
        task_type
)
select * from means
;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cefr_level">
          <DataField>cefr_level</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cefr_order">
          <DataField>cefr_order</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="task_type">
          <DataField>task_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="mean_diff">
          <DataField>mean_diff</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="mean_num_int">
          <DataField>mean_num_int</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
