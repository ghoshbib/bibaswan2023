  <DataSets>
    <DataSet Name="eCommerceTotals">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select datepart(yyyy, bok_when_created) as bookyear, datepart(mm, bok_when_created) as bookmonth, crc_id, SUM(BLN_PRICE) as booktotal from BOOKING_LINES bl WITH (NOLOCK)
inner join BOOKINGS b
on b.BOK_ID = bl.BOK_ID
where bln_price &gt; 0
and BOK_TYPE &lt;&gt; 'INSTITUTION'
AND ((bok_when_created &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, bok_when_created) &lt;= @EndDate OR @EndDate IS NULL))
group by crc_id, datepart(yyyy, bok_when_created), datepart(mm, bok_when_created), crc_id
order by datepart(yyyy, bok_when_created), datepart(mm, bok_when_created)</CommandText>
      </Query>
      <Fields>
        <Field Name="bookyear">
          <DataField>bookyear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="bookmonth">
          <DataField>bookmonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="crc_id">
          <DataField>crc_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="booktotal">
          <DataField>booktotal</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
