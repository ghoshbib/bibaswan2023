    <DataSet Name="Countries">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select cnt_id, cnt_name, cnt_continent, continent_name =
 case cnt_continent
  when 'AF' then 'Africa'
  when 'AN' then 'Antarctica'
  when 'AS' then 'Asia'
  when 'EU' then 'Europe'
  when 'NA' then 'North America'
  when 'OC' then 'Oceania'
  when 'SA' then 'South America'
 end
from countries
where cnt_continent in (@Region)
order by cnt_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cnt_id">
          <DataField>cnt_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cnt_name">
          <DataField>cnt_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cnt_continent">
          <DataField>cnt_continent</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="continent_name">
          <DataField>continent_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Continents">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>select distinct cnt_continent, continent_name =
 case cnt_continent
  when 'AF' then 'Africa'
  when 'AN' then 'Antarctica'
  when 'AS' then 'Asia'
  when 'EU' then 'Europe'
  when 'NA' then 'North America'
  when 'OC' then 'Oceania'
  when 'SA' then 'South America'
 end
from countries
order by cnt_continent</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cnt_continent">
          <DataField>cnt_continent</DataField>
