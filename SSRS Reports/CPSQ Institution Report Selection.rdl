  <DataSets>
    <DataSet Name="AvailableReports">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Course">
            <Value>=Parameters!Course.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FirstName">
            <Value>=Parameters!FirstName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Surname">
            <Value>=Parameters!Surname.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with Candidates as
(
select usr_login, IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , USR_FIRSTNAME, ASN_SOR_FIRSTNAME) AS USR_FIRSTNAME, 
IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME, 
ac.asn_id, ac.ins_id, ins_name, prd_id, crs_name from users u with (nolock)
inner join ASSIGNMENTS a with (nolock)
on a.USR_ID = u.USR_ID
inner join ASSIGNMENT_COURSES ac with (nolock)
on ac.ASN_ID = a.ASN_ID
inner join INSTITUTIONS i with (nolock)
on i.INS_ID = ac.ins_id
inner join courses c with (nolock)
on c.crs_id = ac.crs_id
where prd_id like '%CPSQ%'
and (@Institution IN (SELECT DISTINCT AC.INS_ID FROM ASSIGNMENT_COURSES AC WHERE AC.ASN_ID = A.ASN_ID) OR @Institution = 'CambridgeEnglish')
and (crs_name in (@Course))
)
select c.*, tin.tin_id, tin_when_approved from candidates c
inner join test_instances tin with (nolock)
on tin_assignment_id = asn_id
inner join aggregations agg
on (agg.tin_id = tin.tin_id and agg_id = 'competency_CP')
where ((tin_when_approved &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, TIN_WHEN_APPROVED) &lt;= @EndDate OR @EndDate IS NULL))
and tin_when_approved is not null
and (usr_firstname like @FirstName OR @FirstName = '')
and (usr_surname like @Surname OR @Surname = '')
order by tin_when_approved</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
