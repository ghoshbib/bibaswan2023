  <DataSets>
    <DataSet Name="BMATResults">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Institution">
            <Value>=Parameters!Institution.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with Aptitude as
(
select agg.tin_id, agg.agg_value, a.asn_password, a.asn_token_reference, ASN_SOR_FIRSTNAME, ASN_SOR_SURNAME, ASN_SOR_DOB, ASN_SOR_GENDER, ASN_SOR_EMAIL_ADDRESS, tin_user_id from aggregations agg with (nolock)
inner join test_instances tin with (nolock)
on tin.tin_id = agg.tin_id
inner join assignments a with (nolock)
on a.asn_id = tin_assignment_id
inner join institutions i with (nolock)
on i.ins_id = a.ins_id
where a.ins_id = @Institution
and datepart(YYYY, tin_when_started) = @Year
and agg_name like 'Aptitude%'
),
Knowledge as
(
select agg.tin_id, agg.agg_value from aggregations agg with (nolock)
inner join test_instances tin with (nolock)
on tin.tin_id = agg.tin_id
inner join assignments a with (nolock)
on a.asn_id = tin_assignment_id
inner join institutions i with (nolock)
on i.ins_id = a.ins_id
where a.ins_id = @Institution
and datepart(YYYY, tin_when_started) = @Year
and agg_name like 'Scientific%'
),
Writing as
(
select agg.tin_id, agg.agg_value from aggregations agg with (nolock)
inner join test_instances tin with (nolock)
on tin.tin_id = agg.tin_id
inner join assignments a with (nolock)
on a.asn_id = tin_assignment_id
inner join institutions i with (nolock)
on i.ins_id = a.ins_id
where a.ins_id = @Institution
and datepart(YYYY, tin_when_started) = @Year
and agg_name like 'Writing%'
)
select a.tin_id, a.asn_password, a.asn_token_reference, a.agg_value as A_Score, k.agg_value as K_Score, w.agg_value as W_Score, 
usr_login, IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME, 
ltrim(rtrim(IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , USR_FIRSTNAME, ASN_SOR_FIRSTNAME) + ' ' + IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME))) as Fullname, 
IIF(ISNULL(ASN_SOR_EMAIL_ADDRESS, '') = '' , USR_EMAIL_ADDRESS, ASN_SOR_EMAIL_ADDRESS) AS USR_EMAIL_ADDRESS, 
IIF(ISNULL(ASN_SOR_DOB, '') = '' , USR_DOB, ASN_SOR_DOB) AS USR_DOB from Aptitude a
inner join Knowledge k
on k.tin_id = a.tin_id
inner join Writing w
on w.tin_id = a.tin_id
inner join users u with (nolock)
on u.usr_id = a.tin_user_id
order by a.asn_password</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
