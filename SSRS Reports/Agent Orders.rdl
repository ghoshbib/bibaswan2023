  <DataSets>
    <DataSet Name="Licences">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@productName">
            <Value>=Parameters!productName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@inst">
            <Value>=Parameters!inst.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH insinfo AS (
    SELECT    ii.ins_id,
        ii.ins_name,
        CAST(ii.ins_when_created AS date) AS when_set_up,
        us.usr_firstname,
        us.usr_surname,
        us.usr_login,
        CASE ur.rol_id 
            WHEN 'CEPT_Agent' THEN 'D171'
            WHEN 'Linguaskill_Agent' THEN 'D541'
            WHEN 'Agent' THEN 'D176'
            ELSE 'Unknown'
        END AS prd
FROM    institutions ii
INNER JOIN users us
    ON    ii.ins_id = us.ins_id
        AND
        us.usr_is_student = 0
        AND
        us.usr_is_enabled = 1
INNER JOIN user_roles ur
    ON    us.usr_id = ur.usr_id
        AND
        ur.rol_id LIKE '%Agent'
WHERE   ii.ins_id IN (@inst)
)

SELECT    i1.ins_id AS centre_number,
    i1.ins_name AS centre_name,
    pr.prd_short_description AS product,
    i1.when_set_up,
    il.ins_licences_assigned_locations AS licences_requested,
    i1.usr_firstname AS agent_first_name,
    i1.usr_surname AS agent_surname,
    i1.usr_login AS email_address
FROM    insinfo i1
INNER JOIN    products pr
    ON    i1.prd = pr.prd_id
INNER JOIN    institution_licences il
    ON    i1.prd = il.prd_id
        AND
        i1.ins_id = il.ins_id
WHERE    pr.prd_id IN (@productName)
ORDER BY    i1.ins_id ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
