    <DataSet Name="Institut">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@productName">
            <Value>=Parameters!productName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=DateAdd(DateInterval.Day, -Parameters!prdDays.Value, Parameters!endDate.Value)</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT	ii.ins_id,
		ii.ins_name 
FROM		institutions ii
INNER JOIN	institution_licences il
	ON	ii.ins_id = il.ins_id
WHERE	il.prd_id IN (@productName)
	AND
	ii.ins_when_created &gt;= @startDate
	AND
	ii.ins_when_created &lt;= @endDate
	AND
	ii.ins_is_main_group = 1
ORDER BY	ii.ins_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ins_id">
          <DataField>ins_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ins_name">
          <DataField>ins_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>MetricaDeliveryLive</DataSourceName>
        <CommandText>SELECT DISTINCT	pr.prd_id,
		pr.prd_short_description
FROM	institution_licences_locations_orders ir
INNER JOIN	products pr
	ON	ir.prd_id = pr.prd_id
WHERE	pr.prd_id IN ('D171', 'D541', 'D176')
ORDER BY	pr.prd_short_description ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prd_id">
          <DataField>prd_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prd_short_description">
          <DataField>prd_short_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
