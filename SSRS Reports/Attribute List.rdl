  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MetricaAdminLive</DataSourceName>
        <CommandText>select a.att_id, ATT_IS_FOR_ITEM, ATT_IS_FOR_TEST, ATT_IS_SYSTEM, prj_id, initem = 
 case (SELECT TOP 1 '1' from ITEM_ATTRIBUTES ia where ia.ATT_ID = a.att_id)
  when NULL THEN 0
  when '1' THEN 1
 end,
 intest =
 case (SELECT TOP 1 '1' from TEST_ATTRIBUTES ta where ta.ATT_ID = a.att_id)
  when NULL THEN 0
  when '1' THEN 1
 end
 from attributes a
 left join project_attributes pa
 on pa.att_id = a.att_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="att_id">
          <DataField>att_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ATT_IS_FOR_ITEM">
