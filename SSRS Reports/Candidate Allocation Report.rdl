        <CommandText>select USR_LOGIN, IIF(ISNULL(ASN_SOR_FIRSTNAME, '') = '' , USR_FIRSTNAME, ASN_SOR_FIRSTNAME) AS USR_FIRSTNAME, 
IIF(ISNULL(ASN_SOR_SURNAME, '') = '' , USR_SURNAME, ASN_SOR_SURNAME) AS USR_SURNAME, 
ASN_TIME_ALLOWED, ASN_ACCESS_DETAILS, ASN_PASSWORD, INS_NAME, SGP_NAME, SES_REFERENCE, CNT_TEST_ID, CNT_LAUNCH_DATA, ASN_TEST_INDEX, CA.ATT_VALUE AS OVCOLL from ASSIGNMENTS a
inner join USERS u
on u.USR_ID = a.USR_ID
inner join SESSION_GROUPS sg
on a.SGP_ID = sg.SGP_ID
inner join SESSIONS s
on s.SES_ID = a.SES_ID
inner join CONTENT c
on c.CNT_ID = s.CNT_ID
inner join INSTITUTIONS i
on i.INS_ID = u.INS_ID
LEFT JOIN CUSTOM_ATTRIBUTES CA
ON (CA.USR_ID = U.USR_ID AND CA.ATT_NAME = 'College')
where SGP_NAME like @SessionGroup
AND ((SGP_START_DATE &gt;= @StartDate OR @StartDate IS NULL)
AND (DATEADD(Day, -1, SGP_START_DATE) &lt;= @EndDate OR @EndDate IS NULL))
order by SES_REFERENCE, INS_NAME</CommandText>
