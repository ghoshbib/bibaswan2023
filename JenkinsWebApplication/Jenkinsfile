pipeline {
    agent any
    environment {
        dotnet = 'C:\\Program Files\\dotnet\\dotnet.exe'
    }
    stages {
        stage('Checkout Stage') {
            steps {
                git url: 'https://github.com/ghoshbib/bibaswan2023.git', branch: 'jenkins'
            }
        }
        stage('Build Stage') {
            steps {
                sh 'dotnetBuild project: 'JenkinsWebApplication\\JenkinsWebApplicationDemo.sln', sdk: '.NET 6''
            }
        }
       
        }
    }
