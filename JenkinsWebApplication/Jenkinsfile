pipeline {
    agent {label 'slave'}
    stages {
        stage('Checkout Stage') {
            steps {
                git url: 'https://github.com/ghoshbib/bibaswan2023.git', branch: 'jenkins'
            }
        }
        stage('Build Stage') {
            steps {
                bat "dotnet JenkinsWebApplication/JenkinsWebApplicationDemo.sln /t:Build /p:Configuration='Release'"
            }
        }
       
        }
    }
